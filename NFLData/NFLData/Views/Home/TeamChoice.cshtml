
@{
    ViewBag.Title = "TeamChoice";
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery-1.10.2.min.js")"></script>
    @model NFLData.Models.TeamModel
}

<h2>TeamChoice</h2>
@ViewBag.thingy

<!DOCTYPE html>
<html>
<head>
    <style>
        #map {
            height: 200px;
            width: 100%;
        }
    </style>
</head>
<body>
    <h3>My Google Maps Demo</h3>
    <div id="map"></div>
    <script>
      function initMap() {
          var uluru = { lat: 32.747778, lng: -97.092778 };
          @*var uluru = @Model.StadiumGeo*@
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 13,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAouj0O35FNcvrh1pbfq_ksRGmjDY_WD9I&callback=initMap">
    </script>
</body>
</html>